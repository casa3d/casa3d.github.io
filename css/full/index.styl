#visas_style_div.popup
  display none!important
#info
  position fixed
  z-index 1
@font-face
  font-family awesome
  src url(fontawesome-webfont.woff)
section label
  display block
#fs,#ft img
  transition 1s
#fs,#info,#ok
  font-family awesome
#fs,#info,label,#lg
  font-size 2.2em
input
  font-size .5em
#fs,section *,#ft img
  border-radius 1em
#lg,#lg::after
  border-radius .1em
#lg::before
  bottom 0
  content ' '
  background linear-gradient(black,#00F5FF,black)
  height 10%
  position absolute
  width 46%
#lg::before,input
  transition .4s
#lg::after
  content '\f044\f015'
#fs>section>label:nth-of-type(1)::before
  content '\f007'
#fs>section>label:nth-of-type(2)::before,#fs div>label:nth-of-type(1)::before
  content '\f132'
#ok::before
  content '\f05d'
#lnk::after
  content ' \f14c'
#b
  background repeating-linear-gradient(45deg,rgba(170,170,170,43),rgba(255,255,255,.4),rgba(126,126,126,.8)10%),repeating-linear-gradient(-45deg,#AAA,#DDD,#7E7E7E 10%)
input
  background linear-gradient(rgba(0,0,0,.5),transparent,rgba(0,0,0,.5))
#ok
  background transparent
  cursor pointer
#cv
  background #888
#ok,#ft>div
  font-size 1.5em
#fs *:not(a),#ok
  color #FFF
a
  color #180
  cursor alias
#fs *:focus
  border #09F 4px double
#fs *
  outline-color transparent
#chk
  outline-color #000
.noread
  color #F00
.load
  background radial-gradient(transparent,#FFF,#000)
.ok,#ft img
  border darkgreen 2px dashed
.bad
  border red 2px dashed
.no
  display none
.su
  background linear-gradient(90deg,rgba(0,173,180,.5)45%,transparent 65%)
.lg
  background linear-gradient(90deg,transparent 45%,rgba(0,162,168,.5)65%)
.lg::before
  left 52%
.subg
  background linear-gradient(45deg,rgba(214,255,0,.3),rgba(0,0,0,.5))
.lgbg
  background linear-gradient(45deg,rgba(255,0,0,.15),rgba(0,255,0,.15),rgba(0,0,255,.3)),rgba(0,0,0,.5)
#fs,input,#ft
  text-align center
#fs
  padding .5em
#lg
  cursor move
  left 43%
  position relative
  text-align left
#ft
  font-family verdana
#ft>span
  margin 0 32px
#ft a
  position absolute
#ft img:hover
  height 64px
  width 64px
#ft img:hover
  height 80px
  -webkit-transform rotate3D(1,1,1,-360deg)

.prg::after
  content '% \f05a'
.end::after
  content '\f11e'
.re::after
  content '\f1da'